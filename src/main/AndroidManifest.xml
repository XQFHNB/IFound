<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.anif">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <!--图片选择器权限-->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".atys.AtyEntry"
            android:theme="@style/AppTheme.Base.NoActionBar"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".atys.AtyMain"
            android:theme="@style/AppTheme.Base.NoActionBar"></activity>
        <activity android:name=".atys.AtyEditLogin"></activity>
        <activity android:name=".atys.AtyEditRegister"></activity>
        <activity
            android:name=".atys.AtyPublish"
            android:theme="@style/AppTheme.Base.NoActionBar"></activity>
        <activity
            android:name=".atys.AtyMyManagement"
            android:theme="@style/AppTheme.Base.NoActionBar"></activity>
        <activity
            android:name=".atys.AtyAbout"
            android:theme="@style/AppTheme.Base.NoActionBar"></activity>
    </application>

</manifest>